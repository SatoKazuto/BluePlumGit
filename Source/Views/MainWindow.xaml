<Window x:Class="BluePlumGit.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
        xmlns:l="http://schemas.livet-mvvm.net/2011/wpf"
        xmlns:v="clr-namespace:BluePlumGit.Views"
        xmlns:vm="clr-namespace:BluePlumGit.ViewModels"
        Title="MainWindow" Height="350" Width="525">
    
    <Window.DataContext>
        <vm:MainWindowViewModel/>
    </Window.DataContext>
    
    <Grid>
        <Button Content="Commit" Height="23" HorizontalAlignment="Left" Margin="152,84,0,0" Name="button1" VerticalAlignment="Top" Width="75" Command="{Binding Path=CommitCommand}" />
        <Button Content="Clone" Height="23" HorizontalAlignment="Left" Margin="188,165,0,0" Name="button2" VerticalAlignment="Top" Width="75" Command="{Binding Path=CloneCommand}" />
    </Grid>
</Window>
